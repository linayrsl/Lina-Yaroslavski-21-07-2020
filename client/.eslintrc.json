{
  "extends": [
    "react-app",
    "airbnb-base"
  ],
  "rules": {
    "quotes": ["error", "double"],
    "jsx-quotes": ["error", "prefer-double"],
    "import/prefer-default-export": "off"
  },
  "overrides": [{
    "files": [
      "**/*.js?(x)"
    ],
    "rules": {
      "class-methods-use-this": "off"
    }
  }]
}
